<%- include('../Partial/head.ejs') %>
    <%- include('../Partial/header.ejs') %>
        <div class="body-wrapper">
            <div class="container-fluid">


                                
                <div class="table-responsive rounded-2 mb-4">
                    <table class="table border text-nowrap customize-table mb-0 align-middle">
                      <thead class="text-dark fs-4">
                        <tr>
                          <th><h6 class="fs-4 fw-semibold mb-0">Name</h6></th>
                          <th><h6 class="fs-4 fw-semibold mb-0">Mobile No.</h6></th>
                          <th><h6 class="fs-4 fw-semibold mb-0">Email-id</h6></th>
                          <th><h6 class="fs-4 fw-semibold mb-0">Subject</h6></th>
                          <th><h6 class="fs-4 fw-semibold mb-0">Description</h6></th>
                          <th></th>
                        </tr>
                      </thead>
                      <tbody>


                        <% for( user of data) { %>

                        <tr>

                           
                          <td><p class="mb-0 fw-normal"><%= user.Name %></p>
                        </td>
                          <td>
                            <p class="mb-0 fw-normal"><%= user.Mobile %></p>
                          </td>
                          <td>
                            <p class="mb-0 fw-normal"><%= user.EmailID %></p>
                          </td>
                          <td>
                            <p class="mb-0 fw-normal"><%= user.Subject %></p>
                          </td>
                          <td>
                            <p class="mb-0 fw-normal"><%= user.Description %></p>
                          </td>

                        
                      <% } %>





                        </tr>
                      </tbody>
                    </table>
                  </div>

                
</div>
</div>
<%- include('../Partial/footer.ejs') %>