<%- include('../Partial/head.ejs') %>
<%- include('../Partial/header.ejs') %>


<div class="body-wrapper">
  <div class="container-fluid" style="padding-top: 150px !important;">





    <div class="table-responsive rounded-2 mb-4">
        <table class="table border text-nowrap customize-table mb-0 align-middle">
          <thead class="text-dark fs-4">
            <tr>
              <th><h6 class="fs-4 fw-semibold mb-0">Starting Time</h6></th>
              <th><h6 class="fs-4 fw-semibold mb-0">Ending Time</h6></th>

              <th></th>
            </tr>
          </thead>
          <tbody>
              <% for (d of data){ %>

            <tr>
              <td>

                <div class="d-flex align-items-center">

               

                  <div class="ms-3">
                    
                    <h6 class="fs-4 fw-semibold mb-0"><%= d.StartTime %></h6>
                  </div>
                </div>
              </td>

              <td>
                <div class="d-flex align-items-center">

               

                  <div class="ms-3">
                    <h6 class="fs-4 fw-semibold mb-0"><%= d.EndTime %></h6>
                  </div>
                </div>
                
              </td>
             

                <td>

                  <form action="/editTeacherTimeSlot" method="post">


                    <input type="hidden" name="TeacherTimeSlotDetailID" value="<%= d.TeacherTimeSlotDetailID %>">
              <button type="submit" class="btn justify-content-end btn-primary rounded-pill px-4">
                <div class="d-flex align-items-center">
                  Edit
                </div>
              </button>

            </form>
            </td>
            <td>

              <form action="/remove/<%= d.TeacherTimeSlotDetailID %>" method="post">
              <button type="submit" class="btn justify-content-end btn-danger rounded-pill px-4">
                <div class="d-flex align-items-center">
                  Delete
                </div>
              </button>
            </form>
            </td>

            </tr>

            <% } %>
          </tbody>
        </table>
      </div>





  </div>
</div>


<%- include('../Partial/footer.ejs') %>